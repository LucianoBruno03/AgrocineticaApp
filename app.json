{
  "expo": {
    "name": "Agrocinetica",
    "slug": "Agrocinetica",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/agrocinetica/simpleLogo.png",
    "scheme": "agrocinetica",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
   "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucianodev03.testapp52",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/agrocinetica/simpleLogo.png",
        "backgroundColor": "#ffffff00"
      },
      "package": "com.lucianodev03.testapp52",
      "edgeToEdgeEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/agrocinetica/simpleLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/agrocinetica/simpleLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff00"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
